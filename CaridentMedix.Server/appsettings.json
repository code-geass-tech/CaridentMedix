{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://*:5000",
        "Certificates": {
          "Default": {
            "Path": "~/certs/bundle.crt",
            "KeyPath": "~/certs/server.key"
          }
        }
      }
    }
  }
}
